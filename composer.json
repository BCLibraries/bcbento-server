{
  "name": "bclibraries/bcbento-server",
  "description": "Backend to BC Libraries bento search",
  "authors": [
    {
      "name": "Benjamin Florin",
      "email": "benjamin.florin@bc.edu"
    }
  ],
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "^7.4.0",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-simplexml": "*",
    "bclibraries/fulltext-finder": "^0.1",
    "bclibraries/libkey-client": "^0.1",
    "bclibraries/primo-client": "^0.6",
    "doctrine/annotations": "^1.0",
    "doctrine/doctrine-migrations-bundle": "^2.0",
    "elasticsearch/elasticsearch": "^7.0",
    "myclabs/php-enum": "~1.7",
    "nelmio/cors-bundle": "~1.5",
    "phpdocumentor/reflection-docblock": "^5.3",
    "sensio/framework-extra-bundle": "^5.3",
    "sensiolabs/security-checker": "^6.0",
    "symfony/asset": "4.4.*",
    "symfony/cache": "4.4.*",
    "symfony/console": "4.4.*",
    "symfony/dotenv": "4.4.*",
    "symfony/expression-language": "4.4.*",
    "symfony/flex": "^1.1",
    "symfony/form": "4.4.*",
    "symfony/framework-bundle": "4.4.*",
    "symfony/http-client": "4.4.*",
    "symfony/intl": "4.4.*",
    "symfony/monolog-bundle": "^3.1",
    "symfony/process": "4.4.*",
    "symfony/property-access": "4.4.*",
    "symfony/property-info": "4.4.*",
    "symfony/security-bundle": "4.4.*",
    "symfony/serializer": "4.4.*",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/translation": "4.4.*",
    "symfony/twig-bundle": "4.4.*",
    "symfony/validator": "4.4.*",
    "symfony/web-link": "4.4.*",
    "symfony/yaml": "4.4.*",
    "thecodingmachine/graphqlite-bundle": "^4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/browser-kit": "^4.4",
    "symfony/css-selector": "^4.4",
    "symfony/debug-bundle": "^4.4",
    "symfony/maker-bundle": "^1.11",
    "symfony/phpunit-bridge": "^6.0",
    "symfony/stopwatch": "^4.4",
    "symfony/web-profiler-bundle": "^4.4",
    "symfony/web-server-bundle": "4.4.*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "platform": {
      "php": "7.4.14"
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "4.4.*"
    },
    "symfony-web-dir": "/var/www/html/bcbento-server"
  }
}
